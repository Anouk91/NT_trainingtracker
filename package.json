{
  "name": "nt-training-tracker",
  "version": "1.0.0",
  "description": "A PWA for the national team players to track their exercises. ",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --inline --progress",
    "start": " node ./bin/www",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "e2e": "node test/e2e/runner.js",
    "test": "npm run unit && npm run e2e",
    "lint": "eslint --ext .js,.vue src test/unit/specs test/e2e/specs",
    "build": "node build/build.js"
  },
  "author": "Anouk Boukema & Joram Mossink",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.5",
    "css-loader": "^3.2.0",
    "html-webpack-plugin": "^3.2.0",
    "vue-loader": "^15.7.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "bluebird": "^3.7.1",
    "bootstrap": "^4.3.1",
    "bootstrap-vue": "^2.1.0",
    "cors": "^2.8.5",
    "mongoose": "^5.7.12",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3"
  }
}
